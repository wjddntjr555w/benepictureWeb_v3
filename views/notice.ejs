<!DOCTYPE html>
<html>

<head> <% include ./static/head %> </head>

<body>
<header> <% include ./static/header %> </header>

<section id="subtitle">
    <div id="subtitle_box"> <%= subtitle%> </div>
</section>

<section id="main_section">
    <table class="table table-hover">
        <thead>
        <tr>
            <th>번호</th>
            <th>제목</th>
            <th>날짜</th>
        </thead>
        <tbody>
        <% var j=data.length;%>
        <% for(var i=0;i<data.length;i++){ %>
        <tr>
            <td> <%= j-i %></td>
            <td><a href="/notice/<%= data[i]._id %>"><%= data[i].title %> </a></td>
            <td><%= moment(data[i].createdAt).format('YYYY-MM-DD') %></td>
        </tr>
        <% }%>
        </tbody>
    </table>
</section>

<footer> <% include ./static/footer %> </footer>

</body>

</html>
